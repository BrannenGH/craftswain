{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "lint",
      "type": "eslint",
      "problemMatcher": ["$eslint-stylish"]
    },
    {
      "label": "build",
      "type": "npm",
      "script": "build",
      "problemMatcher": ["$tsc"],
      "dependsOn": ["lint"]
    },
    {
      "label": "run e2e tests",
      "type": "npm",
      "script": "start",
      "dependsOn": ["build"],
      "problemMatcher": []
    }
  ]
}
