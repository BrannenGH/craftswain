{
  "version": "1.0.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Jest: current file",
      "env": { "DEBUG": "*" },
      "runtimeExecutable": "yarn",
      "runtimeArgs": ["run", "--inspect-brk", "jest"],
      "args": ["${fileBasenameNoExtension}", "--config", "out/jest.config.js"],
      "console": "internalConsole",
      "preLaunchTask": "build"
    }
  ]
}
