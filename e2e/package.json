{
  "name": "craftswain-test-scripts",
  "version": "0.0.0",
  "packageManager": "yarn@3.4.1",
  "description": "A sandbox to test various quality assurance automation solutions",
  "main": "index.js",
  "scripts": {
    "build": "tsc && shx cp craftswain.config.yaml out/",
    "start": "jest -c out/jest.config.js --runInBand --detectOpenHandles --forceExit",
    "clean": "shx rm -rf out/ > /dev/null"
  },
  "author": "Brannen Hall",
  "license": "MIT",
  "dependencies": {
    "@craftswain/core": "workspace:^0.1.0",
    "@craftswain/docker": "workspace:^0.1.0",
    "@craftswain/jest": "workspace:^0.1.0",
    "@craftswain/selenium": "workspace:^0.1.0",
    "jest": "^29.4.3",
    "jest-environment-node": "^29.4.3",
    "selenium-webdriver": "^4.8.1",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@jest/globals": "^29.4.3",
    "@types/dockerode": "^3.3.14",
    "@types/node": "^18.14.2",
    "@types/selenium-webdriver": "^4.1.12",
    "eslint": "^8.35.0",
    "shx": "^0.3.4",
    "typescript": "^4.9.5"
  },
  "private": true
}
