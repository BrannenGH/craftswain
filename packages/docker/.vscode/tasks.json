{
  "version": "2.0.0",
  "tasks": [
    {
      "label": "lint",
      "type": "eslint",
      "problemMatcher": ["$eslint-stylish"]
    },
    {
      "label": "build",
      "type": "npm",
      "script": "build",
      "problemMatcher": ["$tsc"],
      "dependsOn": ["lint"],
    },
    {
      "label": "test",
      "type": "npm",
      "script": "test",
      "dependsOn": ["build"],
      "problemMatcher": []
    },
    {
      "label": "e2e",
      "type": "npm",
      "script": "e2e",
      "dependsOn": ["build"],
      "problemMatcher": []
    }
  ]
}
